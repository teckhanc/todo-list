<%- include("partials/header") -%>
<div class="box">
  <h1> <%= todoName %> </h1>
  <% for (var i = 0; i < items.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p> <%=items[i]%> </p>
    </div>
  <% } %>
  <form class="" action="/list/<%=listId%>" method="post">
    <input type="text" name="items" value="" placeholder="Todo">
    <button type="Submit" name="create" value="item">Add to my List</button>
  </form>
</div>
<%- include("partials/footer") -%>
