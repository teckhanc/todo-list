<%- include("partials/header") -%>
<div class="box">
  <h1> <%= todoName %> </h1>
  <% for (var i = 0; i < items.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p> <%=items[i]%> </p>
    </div>
  <% } %>
  <form action="/list/<%=listId%>" method="post">
    <input class="text-input form-control" type="text" name="items" placeholder="Todo">
    <button class="btn btn-primary" type="Submit">Add</button>
  </form>
</div>
<%- include("partials/footer") -%>
