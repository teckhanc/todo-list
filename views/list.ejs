<%- include("partials/header") -%>
    <h1> <%= todoName %> </h1>
    <ul>
      <% for (var i = 0; i < items.length; i++) { %>
        <li> <%=items[i]%> </li>
      <% } %>
    </ul>
    <form class="" action="/list/<%=todoName%>" method="post">
      <input type="text" name="items" value="" placeholder="Todo">
      <button type="Submit" name="create" value="item">Add to my List</button>
    </form>

<%- include("partials/footer") -%>
